{
  "manifest_version": 2,

  "name": "randoFy ",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "helloKitty.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"

  ]
}